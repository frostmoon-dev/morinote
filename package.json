{
  "name": "morinote",
  "version": "1.0.0",
  "description": "Electron documentation app for developers with ADHD",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron": "electron .",
    "package": "electron-builder -w"
  },
  "build": {
    "appId": "com.morinote.app",
    "productName": "Morinote",
    "win": {
      "target": "nsis"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "package.json",
      "node_modules/**/*"
    ]
  },
  "author": "User",
  "license": "MIT",
  "dependencies": {
    "easymde": "^2.20.0",
    "lowdb": "^7.0.0",
    "lucide-react": "^0.300.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-simplemde-editor": "^5.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "electron": "^33.0.0",
    "electron-builder": "^25.0.0",
    "jest": "^29.0.0",
    "vite": "^5.0.0"
  }
}
